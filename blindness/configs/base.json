{
    "name": "base_test",
    "input": {
        "width": 288,
        "height": 288,
        "transforms": [
            "random_resized_crop"
        ],
        "test_transforms": [
            "random_resized_crop"
        ]
    },
    "dataset": {
        "fold": 0,
        "num_class": 5,
        "batch_size": 32,
        "num_workers": 4,
        "method":"classification",
        "use_diabetic_retinopathy": true
    },
    "model": {
        "name": "resnet50",
        "pretrained": false,
        "weight_path": "../input/resnet50/resnet50.pth"
    },
    "train_param": {
        "epoch": 20,
        "lr": 0.0001,
        "grad_clip_step": 20,
        "grad_clip": 100.0,
        "scheduler": "cosine",
        "cosine": {
            "t_max": 20,
            "eta_min": 0
        },
        "steplr": {
            "step_size": 4,
            "gamma": 0.1
        }
    }
}